   select UnitPrice * Quantity * (1-Discount) as price, o.OrderDate
   from [Order Details] od
   join Orders o
   on o.OrderID = od.OrderID
   where year(o.OrderDate) = 1996

   --------------------------------

     select sum(UnitPrice * Quantity * (1-Discount)) as price
   from [Order Details] od
   join Orders o
   on o.OrderID = od.OrderID
   where year(o.OrderDate) = 1996
